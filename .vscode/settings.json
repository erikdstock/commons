{
  "cSpell.words": [
    "Plex",
    "docz",
    "flexbox",
    "monospace",
    "theming"
  ],
  "prettier.semi": false,
  "editor.formatOnSave": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.autoSave": "onFocusChange",
  "tslint.enable": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    { "language": "typescript", "autoFix": true },
    { "language": "typescriptreact", "autoFix": true }
  ]
}
